{
    "name": "thefox/tumblr-parser",
    "description": "A Tumblr Theme parser in PHP.",
    "license": "GPL-3.0",
    "type": "library",
    "keywords": [
        "tumblr",
        "theme",
        "parser"
    ],
    "homepage": "https://fox21.at",
    "authors": [
        {
            "name": "Christian Mayer",
            "email": "christian@fox21.at",
            "homepage": "https://fox21.at"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/yaml": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "phpstan/phpstan": "^0.7",
        "thefox/utilities": "^1.1"
    },
    "autoload": {
        "psr-0": {
            "TheFox\\": "src"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "TheFox\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x.x-dev"
        }
    }
}

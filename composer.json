{
	"name": "thefox/tumblr-parser",
	"description": "A Tumblr Theme parser in PHP.",
	"license": "GPL-3.0",
	"type": "library",
	"keywords": [ "tumblr", "theme", "parser" ],
	"homepage": "http://fox21.at",
	"authors": [
		{
			"name": "Christian Mayer",
			"email": "christian@fox21.at",
			"homepage": "http://fox21.at"
		}
	],
	"require": {
		"php": ">=5.3",
		"symfony/yaml": "~2.4"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.1",
		"doctrine/common": "~2.4",
		"thefox/phpcsrs": "~1.0",
		"thefox/utilities": "~1.1"
	},
	"autoload": {
		"psr-0": { "TheFox": "src" }
	},
	"autoload-dev": {
		"psr-0": { "TheFox": "src" }
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.x.x-dev"
		}
	},
	"scripts": {
		"post-update-cmd": [
			"echo post-update-cmd",
			"php bootstrap.php"
		]
	}
}